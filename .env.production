# Production Environment Configuration

# API Configuration
API_BASE_URL=https://api.example.com/api
API_TIMEOUT=20000
API_RETRIES=3

# Feature Flags
FEATURE_NEW_UI=true
FEATURE_ANALYTICS=true
FEATURE_USER_PROFILES=true

# Logging
LOG_LEVEL=error

# Build Configuration
PUBLIC_URL=/
NODE_ENV=production

# Performance and Caching
CACHE_TTL=3600
ENABLE_SERVICE_WORKER=true